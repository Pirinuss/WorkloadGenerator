             {"server":"localhost", "port":11000},
             {"server":"127.0.0.1", "port":11010},
             {"server":"localhost", "port":11020},
             {"server":"localhost", "port":11030}
         ]
        }
  ],     
  "requests": [
        {"id":"request1",
         "protocol":"BFTSMaRt",
         "command": {
            "type":"ByteArray",
